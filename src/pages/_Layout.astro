---
import Head from "./_Head.astro";
import { website } from "./seo";

type Props = {
  title: string;
  description: string;
};

const { title, description } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <Head title={`${title} | ${website.name}`} description={description} />
  </head>
  <body>
    <slot />
  </body>
</html>
