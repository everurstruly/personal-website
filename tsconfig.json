{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@lib/*": ["./src/lib/*"],
      "@pages/*": ["./src/pages/*"],
      "@content/*": ["./src/content/*"],
      "@features/*": ["./src/features/*"],
      "@components/*": ["./src/lib/components/*"]
    }
  }
}
